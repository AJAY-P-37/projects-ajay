{
  "name": "projects-ajay",
  "version": "1.0.0",
  "description": "All projects of Ajay",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "frontend:dev": "turbo dev --filter=frontend",
    "frontend:build": "turbo build --filter=frontend",
    "shadcn:dev": "turbo dev --filter=shadcn-lib",
    "shadcn:build": "turbo build --filter=shadcn-lib",
    "app:dev": "turbo dev --filter=frontend",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/src/**/*.{js,ts,tsx,json,css,scss}\"",
    "backend:dev": "turbo dev --filter=backend"
  },
  "repository": "git+https://github.com/AJAY-P-37/projects-ajay.git",
  "keywords": [
    "ajay",
    "personal",
    "projects",
    "37",
    "AJAY-P-37"
  ],
  "author": "Ajay Prahalathan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJAY-P-37/projects-ajay/issues"
  },
  "homepage": "https://github.com/AJAY-P-37/projects-ajay#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/cli": "^4.1.12",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-dom": "^1.52.3",
    "eslint-plugin-react-x": "^1.52.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.6",
    "turbo": "^2.5.5"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.2",
    "@tanstack/react-query": "^5.90.5",
    "framer-motion": "^12.23.25",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5",
    "tough-cookie": "4"
  }
}
